#!/usr/bin/env python

from Corellia import Worker as CWorker
from Briareus import Worker as BWorker

if __name__ == '__main__':
	execfile("config")
	CLIENT_ADDR = (PROXY_CLIENTEND_ADDR, PROXY_CLIENTEND_PORT)
	WORKER_ADDR = (PROXY_WORKEREND_ADDR, PROXY_WORKEREND_PORT)
	CWorker(BWorker, CLIENT_ADDR).run(WORKER_ADDR)
